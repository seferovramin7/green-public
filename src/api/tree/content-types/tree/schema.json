  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "inserted_date": {
      "type": "date"
    },
    "updated_date": {
      "type": "date"
    },
    "price": {
      "type": "decimal"
    },
    "current_location": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "media_asset": {
      "type": "text"
