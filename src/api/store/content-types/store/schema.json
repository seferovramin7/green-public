{
  "kind": "collectionType",
  "collectionName": "stores",
  "info": {
    "singularName": "store",
    "pluralName": "stores",
    "displayName": "Store",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "storeName": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "Address": {
      "type": "string"
    },
    "storeDescription": {
      "type": "text"
    },
    "distance": {
      "type": "float"
    },
    "openingAt": {
      "type": "time"
    },
    "closingAt": {
      "type": "time"
    },
    "availableTrees": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tree.tree",
      "inversedBy": "sellingStores"
    },
    "availableProducts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::care-product.care-product",
      "inversedBy": "sellingStores"
    },
    "openDays": {
      "type": "customField",
      "options": [
        "Monday",
        "Tuesday",
        "Wednesday",
        "Thursday",
        "Friday",
        "Saturday",
        "Sunday"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "coverPhoto": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
