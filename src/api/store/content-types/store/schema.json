  },
  "pluginOptions": {},
  "attributes": {
    "storeName": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "Address": {
      "type": "string"
    },
    "storeDescription": {
      "type": "text"
    },
    "openingAt": {
      "type": "time"
    },
    "closingAt": {
      "type": "time"
    },
